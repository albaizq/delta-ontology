    <!-- http://delta.linkeddata.es/def/core#Aggregator -->


    <owl:Class rdf:about="http://delta.linkeddata.es/def/core#Aggregator">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://delta.linkeddata.es/def/core#consumes"/>
                <owl:someValuesFrom rdf:resource="http://delta.linkeddata.es/def/core#MarketSettlement"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://delta.linkeddata.es/def/core#generates"/>
                <owl:someValuesFrom rdf:resource="http://delta.linkeddata.es/def/core#Bid"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://delta.linkeddata.es/def/core#sendBids"/>
                <owl:someValuesFrom rdf:resource="http://delta.linkeddata.es/def/core#EnergyMarket"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://delta.linkeddata.es/def/core#hasProfile"/>
                <owl:allValuesFrom rdf:resource="http://delta.linkeddata.es/def/core#Profile"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://delta.linkeddata.es/def/core#hasTransaction"/>
                <owl:allValuesFrom rdf:resource="http://delta.linkeddata.es/def/core#Transaction"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Legal entities that aggregate the load or generation of various demand and/or generation/production units and aim at optimizing energy supply and consumption.</rdfs:comment>
        <rdfs:label>Aggregator</rdfs:label>
    </owl:Class>
    


    <!-- http://delta.linkeddata.es/def/core#Bid -->


    <owl:Class rdf:about="http://delta.linkeddata.es/def/core#Bid">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://delta.linkeddata.es/def/core#isValidDuringPeriod"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2006/time#TemporalEntity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Bid that is estimated by the aggregator</rdfs:comment>
        <rdfs:label>Bid</rdfs:label>
    </owl:Class>
    


    <!-- http://delta.linkeddata.es/def/core#Cluster -->


    <owl:Class rdf:about="http://delta.linkeddata.es/def/core#Cluster">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://delta.linkeddata.es/def/core#hasAggregatedProperty"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/saref#Property"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://delta.linkeddata.es/def/core#clusterDescription"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://delta.linkeddata.es/def/core#clusterName"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Aggrupation of elements, such as FEIDs or Virtual Nodes</rdfs:comment>
        <rdfs:label>Cluster</rdfs:label>
    </owl:Class>
    


    <!-- http://delta.linkeddata.es/def/core#ComfortSetting -->


    <owl:Class rdf:about="http://delta.linkeddata.es/def/core#ComfortSetting">
        <rdfs:label>Comfort setting</rdfs:label>
    </owl:Class>
    


    <!-- http://delta.linkeddata.es/def/core#Consumer -->


    <owl:Class rdf:about="http://delta.linkeddata.es/def/core#Consumer">
        <rdfs:subClassOf rdf:resource="http://delta.linkeddata.es/def/core#Prosumer"/>
        <rdfs:comment>Entity that consumes energy</rdfs:comment>
        <rdfs:label>Consumer</rdfs:label>
    </owl:Class>
    


    <!-- http://delta.linkeddata.es/def/core#Customer -->


    <owl:Class rdf:about="http://delta.linkeddata.es/def/core#Customer">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://delta.linkeddata.es/def/core#hasReward"/>
                <owl:allValuesFrom rdf:resource="http://delta.linkeddata.es/def/core#Reward"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://delta.linkeddata.es/def/core#type"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2002/07/owl#rational"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Entity that interacts in the platform</rdfs:comment>
        <rdfs:label>Customer</rdfs:label>
    </owl:Class>
    


    <!-- http://delta.linkeddata.es/def/core#DRSignal -->


    <owl:Class rdf:about="http://delta.linkeddata.es/def/core#DRSignal">
        <rdfs:subClassOf rdf:resource="http://delta.linkeddata.es/def/core#Signal"/>
        <rdfs:comment>Signal sent by entities (e.g., virutal node or aggregator) related to a DR action. These signals are aligned with OpenADR standard.</rdfs:comment>
        <rdfs:label>DR Signal</rdfs:label>
    </owl:Class>
    


    <!-- http://delta.linkeddata.es/def/core#Device -->


    <owl:Class rdf:about="http://delta.linkeddata.es/def/core#Device">
        <rdfs:subClassOf rdf:resource="https://w3id.org/saref#Device"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://delta.linkeddata.es/def/core#belongsTo"/>
                <owl:someValuesFrom rdf:resource="http://delta.linkeddata.es/def/core#FEID"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://delta.linkeddata.es/def/core#isOwnedBy"/>
                <owl:someValuesFrom rdf:resource="http://delta.linkeddata.es/def/core#Customer"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://delta.linkeddata.es/def/core#Energy -->


    <owl:Class rdf:about="http://delta.linkeddata.es/def/core#Energy">
        <rdfs:subClassOf rdf:resource="https://w3id.org/saref#Property"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://delta.linkeddata.es/def/core#hasIncentive"/>
                <owl:allValuesFrom rdf:resource="http://delta.linkeddata.es/def/core#Incentive"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A property related to measurements that are characterized  by a value measured in energy units</rdfs:comment>
        <rdfs:label>Energy</rdfs:label>
    </owl:Class>
    


    <!-- http://delta.linkeddata.es/def/core#EnergyMarket -->


    <owl:Class rdf:about="http://delta.linkeddata.es/def/core#EnergyMarket">
        <rdfs:label>Energy Market</rdfs:label>
    </owl:Class>
    


    <!-- http://delta.linkeddata.es/def/core#FEID -->


    <owl:Class rdf:about="http://delta.linkeddata.es/def/core#FEID">
        <rdfs:subClassOf>
